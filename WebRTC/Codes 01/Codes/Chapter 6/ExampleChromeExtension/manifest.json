{
   "author": "Agility Feat, Inc",
   "background": {
      "persistent": false,
      "scripts": [ "background-script.js" ]
   },
   "content_scripts": [ {
      "js": [ "content-script.js" ],
      "matches": [ "*://*/*" ]
   } ],
